<%# "いいねランキング サイドバー" %>
<table class="table">
  <thead>
    <tr>
      <th>順位</th>
      <th>id</th>
      <th>タイトル</th>
      <th>アーティスト名</th>
      <th>いいね数</th>
    </tr>
  </thead>
  <% likes_ranks.zip(likes_ranks_count,ranks_number).each do |rank,count,number| %>
    <tbody>
      <tr>
        <td><%= number %></td>
        <td><%= rank.id %></td>
        <td><%= rank.name %></td>
        <td><%= rank.artist.name %></td>
        <td><%= count %></td>
      </tr>
    </tbody>
  <% end %>
</table>
