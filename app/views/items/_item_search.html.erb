 <section class="search">
  <div class="search-form">
  <%= form_tag(search_path, method: :get) do %>
    <p><%= label :artist_id, "アーティスト" %></p>
    <p><%= collection_select(:artist, :id, artists_search, :id, :name,:include_blank => '指定なし') %></p>
    <p><%= label :label_id, "レーベル" %></p>
    <p><%= collection_select(:label, :id, labels_search, :id, :name,:include_blank => '指定なし') %></p>
    <p><%= label :genre_id, "ジャンル" %></p>
    <p><%= collection_select(:genre, :id, genres_search, :id, :name,:include_blank => '指定なし') %></p>
    <p><%= label :name, "タイトル" %></p>
    <p><%= text_field_tag :name %></p>
    <p><%= submit_tag "検索" %></p>
    <% end %>
  </div>
</section>